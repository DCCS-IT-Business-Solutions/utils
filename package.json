{
  "name": "utils",
  "version": "0.1.1",
  "description": "",
  "main": "preprocessor.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "run-s lint test transpile",
    "transpile": "tsc",
    "lint": "tslint ./src/**/*.ts*",
    "test": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --coverage --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^23.3.13",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "c**/?(*.)(spec|test).ts?(x)"
    ]
  }
}
